<UserControl x:Class="Vision.View.ImagePanel.ROIPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Vision.View.ImagePanel"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <WindowsFormsHost Grid.Row="1" Background="WhiteSmoke" Width="80" Height="80" >
            <local:ROIPanel_winform x:Name="WF_ImagePanel" x:FieldModifier="public"
                                    Width="80" Height="80" />                                   
                                  
        </WindowsFormsHost>
    </Grid>          
</UserControl>
